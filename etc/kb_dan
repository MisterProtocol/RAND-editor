<esc><esc>:<ret>
<kbinit>:<esc>"="
<kbend>:<esc>">"
^M:<ret>
#
# F1 thru F12
<del>:<bksp>
#<esc>"OP":<close>
KEY_F1:<close>
#<esc>"OQ":<cmd><close>
KEY_F2:<cmd><close>
#<esc>"OR":<cmd><dchar>
KEY_F3:<cmd><dchar>
#<esc>"OS":<cmd>"-er"<ret>
KEY_F4:<cmd>"-er"<ret>
#<esc>"[15~":<split>
KEY_F5:<split>
<esc>"[17~":<join>
KEY_F6:<join>
#<esc>"[18~":<pick>
KEY_F7:<pick>
<esc>"[19~":<cmd><pick>
KEY_F8:<cmd><pick>
#<esc>"[20~":<play>
KEY_F9:<play>
#<esc>"[21~":<open>
KEY_F10:<open>
#<esc>"[23~":<-page>
KEY_F11:<-page>
<esc>"[24~":<+page>
KEY_F12:<+page>
#
<esc>"[A":<up>
KEY_UP:<up>
<esc>"[B":<down>
KEY_DOWN:<down>
<esc>"[C":<right>
KEY_RIGHT:<right>
<esc>"[D":<left>
KEY_LEFT:<left>
#
#<esc>"OF":<cmd><right>
KEY_END:<cmd><right>
<esc>"OH":<home>
KEY_HOME:<home>
<esc>"[2~":<insmd>
KEY_IC:<insmd>
<esc>"[1~":<home>
#<esc>"[5~":<-page>
KEY_PPAGE:<-page>
#<esc>"[3~":<dchar>
KEY_DC:<dchar>
<esc>"[4~":<cmd><right>
#<esc>"[6~":<+page>
KEY_NPAGE:<+page>
#esc>"[P":<right>
KEY_BREAK:<cmd>
#
# keypad
<esc>"Oo":"/"
<esc>"Oj":"*"
<esc>"Om":"-"
#esc>"[1~":<home>
<esc>"OA":<up>
#esc>"[5~":<-page>
<esc>"Ok":"+"
<esc>"OD":<left>
<esc>"[E":""
<esc>"OC":<right>
#esc>"[4~":<cmd><right>
<esc>"OB":<down>
<esc>"OE":
#esc>"[5~":<+page>
<esc>"OM":<ret>
KEY_ENTER:<ret>
#esc>"[2~":<ins>
#esc>"[3~":<dchar>
# ctrl keypad
#
<esc>"[53~":<-sch>
<esc>"[54~":<+sch>
<esc>"[56~":<+line>
<esc>"[70~":<-line>
#
^A:<cmd>
^D:<cmd>
^F:<cmd>
^C:<cchar>
^E:<cmd>"e"<ret>
^H:<bksp>
^I:<tab>
^R:<record>
^J:<mark>
^K:<mark>
^L:<mark>
^V:<cmd>"exit"<ret>
^W:<chwin>
^X:<+sch>
^Z:<-sch>
