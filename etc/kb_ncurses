<del>:<bksp>
^@:<cmd>
^A:<cmd>
^B:<-word>
^C:<left>
^D:<-line>
^E:<-page>
^F:<+line>
^G:<home>
#^H:<bksp>
^H:<left>
^I:<tab>
^J:<down>
^K:<up>
^L:<right>
^M:<ret>
^N:<+word>
^O:<open>
^P:<pick>
^R:<+page>
^T:<-sch>
^U:<mark>
^V:<close>
^W:<dchar>
^Y:<+sch>
^Z:<insmd>
^\:<int>
^]:<replace>
#^]:<ret>
^^:<ccase>
^_:<edit>
^X^A:<edit>
^X^B:<split>
^X^C:<cchar>
^X^D:<dword>
^X^E:<erase>
#^X^E:<cmd>"exit"<ret>
^X^F:<redraw>
^X^H:<wleft>
^X^I:<-tab>
^X^J:<join>
^X^L:<wright>
^X^N:<dword>
#^X^R:<replace>
^X^P:<play>
^X^R:<record>
^X^T:<srtab>
^X^U:<-tab>
^X^W:<chwin>
#^X^X:<cchar>
#
# raw xterm codes (0411-0454)
#
KEY_F1:<cmd>
KEY_F2:<redraw>
KEY_F3:<+word>
KEY_F4:<-word>
KEY_F5:<replace>
/* F6:  toggle mouse on/off */
KEY_F6:<mouse>
# etc.
#
SHIFT_F1:<+page>
SHIFT_F2:<-page>
SHIFT_F3:<caps>
SHIFT_F4:<ccase>
SHIFT_F5:<insmd>
# etc
#
CTRL_F1:<split>
CTRL_F2:<join>
CTRL_F3:<mouse>
CTRL_F4:<record>
CTRL_F5:<play>
# etc
# ALT-m : toggle mouse on/off
<esc>"m":<mouse>
